<!DOCTYPE html>
<html>
<head>
	<title>Home Page</title>
</head>
<body>
<center>
	<div>
		<h1>User List</h1> 		
	</div>

	<a href="/home"> Back</a> |
	<a href="/logout"> logout</a><br>

	<table border="1">
		<tr>
			<td>ID</td>
			<td>Username</td>
			<td>Password</td>
			<td>Type</td>
			<td>Action</td>
		</tr>
		<% for(var i=0; i < userlist.length; i++){ %>
		<tr>
			<td><%= userlist[i].id %></td>
			<td><%= userlist[i].username %></td>
			<td><%= userlist[i].name %></td>
			<td><%= userlist[i].contact %></td>
			<td><a href="/home/edit/<%= userlist[i].username %>">Edit</a>|<a href="/home/delete/<%= userlist[i].id %>">Delete</a></td>

		</tr>
		<% } %>
	</table>
</center>
</body>
</html>